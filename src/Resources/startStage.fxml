<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane fx:id="startPane" prefHeight="600.0" prefWidth="900.0"  xmlns="http://javafx.com/javafx/1.8.0_251" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Java.GUI.FXMLControllers.startStageController">
   <center>
      <Pane prefHeight="600.0" prefWidth="900.0" style="-fx-background-image: url(tlo.jpg)" BorderPane.alignment="CENTER">
         <children>
            <Pane layoutX="360.0" layoutY="220.0" prefHeight="270.0" prefWidth="200.0" style="-fx-border-color: black; -fx-background-color: white; ">
               <children>
                  <Button fx:id="startButton" layoutX="20.0" layoutY="25.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="160.0" style="-fx-font-weight: bold; -fx-font-size: 16px;" text="Start" />
                  <Button fx:id="exitButton" layoutX="20.0" layoutY="194.0" mnemonicParsing="false" onAction="#Exit" prefHeight="50.0" prefWidth="160.0" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Exit" />
                  <Button fx:id="informationButton" layoutX="20.0" layoutY="110.0" mnemonicParsing="false" onAction="#GoToInformation" prefHeight="50.0" prefWidth="160.0" style="-fx-font-weight: bold; -fx-font-size: 16px;" text="Informacje" />
               </children>
            </Pane>
            <Pane layoutX="360.0" layoutY="60.0" prefHeight="90.0" prefWidth="200.0" style="-fx-background-color: white; -fx-border-color: black;">
               <children>
                  <Label layoutX="35.0" layoutY="8.0" style="-fx-font-weight: bold; -fx-font-size: 48px;" text="Menu" />
               </children>
            </Pane>
         </children>
      </Pane>
   </center>
</BorderPane>
